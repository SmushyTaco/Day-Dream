{
  "schemaVersion": 1,
  "id": "day_dream",
  "version": "${version}",
  "name": "Day Dream",
  "description": "This mod allows you to sleep during the day to make it night.",
  "authors": [
    "SmushyTaco"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/day-dream/issues/",
    "sources": "https://github.com/SmushyTaco/Day-Dream/"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "icon": "assets/day_dream/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.day_dream.DayDream"
      }
    ]
  },
  "mixins": [
    "day_dream.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}",
    "owo": ">=${owo_version}"
  }
}